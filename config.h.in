#ifndef THTK_CONFIG_H_
#define THTK_CONFIG_H_

#ifdef _MSC_VER
# define PRAGMA(x) __pragma(x)
#else
# define PRAGMA(x) _Pragma(#x)
#endif

#cmakedefine PACK_PRAGMA
#ifdef PACK_PRAGMA
# define PACK_BEGIN PRAGMA(pack(push,1))
# define PACK_END PRAGMA(pack(pop))
#else
# define PACK_BEGIN
# define PACK_END
#endif

#define PACK_ATTRIBUTE @PACK_ATTRIBUTE@

#cmakedefine WITH_LIBPNG_SOURCE
#cmakedefine PNG_FOUND
#if defined(WITH_LIBPNG_SOURCE) || defined(PNG_FOUND)
# define HAVE_LIBPNG
#endif

#cmakedefine HAVE_OFF_T
#cmakedefine HAVE_SIZE_T
#cmakedefine HAVE_SIZE_T_BASETSD
#if !defined(HAVE_SIZE_T) && defined(HAVE_SIZE_T_BASETSD)
# include <BaseTsd.h>
  typedef SIZE_T size_t;
#endif
#cmakedefine HAVE_SSIZE_T
#cmakedefine HAVE_SSIZE_T_BASETSD
#if !defined(HAVE_SSIZE_T) && defined(HAVE_SSIZE_T_BASETSD)
# include <BaseTsd.h>
  typedef SSIZE_T ssize_t;
#endif
#cmakedefine HAVE_SYS_TYPES_H
#cmakedefine HAVE_STDINT_H
#cmakedefine HAVE_SYS_STAT_H
#cmakedefine HAVE_SYS_MMAN_H
#cmakedefine HAVE_UNISTD_H
#ifndef HAVE_UNISTD_H
# define YY_NO_UNISTD_H
#endif
#cmakedefine HAVE_MEMPCPY
#cmakedefine HAVE_MMAP
#cmakedefine HAVE_MUNMAP
#cmakedefine HAVE_FILENO

#cmakedefine HAVE_FEOF_UNLOCKED
#cmakedefine HAVE_FILENO_UNLOCKED
#cmakedefine HAVE_FREAD_UNLOCKED
#cmakedefine HAVE_FWRITE_UNLOCKED
#cmakedefine HAVE_GETC_UNLOCKED
#cmakedefine HAVE_PUTC_UNLOCKED
#cmakedefine HAVE__FREAD_NOLOCK
#cmakedefine HAVE__FWRITE_NOLOCK
#cmakedefine HAVE__GETC_NOLOCK
#cmakedefine HAVE__PUTC_NOLOCK
#ifndef HAVE_FEOF_UNLOCKED
# define feof_unlocked feof
#endif
#if defined(HAVE_FILENO) && !defined(HAVE_FILENO_UNLOCKED)
# define fileno_unlocked fileno
#endif
#ifndef HAVE_FREAD_UNLOCKED
# ifdef HAVE__FREAD_NOLOCK
#  define fread_unlocked _fread_nolock
# else
#  define fread_unlocked fread
# endif
#endif
#ifndef HAVE_FWRITE_UNLOCKED
# ifdef HAVE__FWRITE_NOLOCK
#  define fwrite_unlocked _fwrite_nolock
# else
#  define fwrite_unlocked fwrite
# endif
#endif
#ifndef HAVE_GETC_UNLOCKED
# ifdef HAVE__GETC_NOLOCK
#  define getc_unlocked _getc_nolock
# else
#  define getc_unlocked getc
# endif
#endif
#ifndef HAVE_PUTC_UNLOCKED
# ifdef HAVE__PUTC_NOLOCK
#  define putc_unlocked _putc_nolock
# else
#  define putc_unlocked putc
# endif
#endif
/* Our code shouldn't need to check these */
#undef HAVE_FEOF_UNLOCKED
#undef HAVE_FILENO_UNLOCKED
#undef HAVE_FREAD_UNLOCKED
#undef HAVE_FWRITE_UNLOCKED
#undef HAVE_GETC_UNLOCKED
#undef HAVE_PUTC_UNLOCKED
#undef HAVE__FREAD_NOLOCK
#undef HAVE__FWRITE_NOLOCK
#undef HAVE__GETC_NOLOCK
#undef HAVE__PUTC_NOLOCK

#ifdef _WIN32
# define API_SYMBOL __declspec(dllexport)

# include <io.h>
# include <fcntl.h>
#endif

#define PACKAGE_NAME "@PROJECT_NAME@"
#define PACKAGE_VERSION "@PROJECT_VERSION@"
#define PACKAGE_BUGREPORT "@PROJECT_BUGREPORT@"

#endif
